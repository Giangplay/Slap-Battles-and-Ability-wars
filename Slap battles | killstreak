Tab3:AddToggle({
	Name = "Normal Arena Anti-Void",
	Default = false,
	Callback = function(state)
		if state then
	jesus.CanCollide = true
	jesus.Anchored = true
	jesus.Transparency = 0.5
    else

	jesus.CanCollide = false
	jesus.Anchored = true
	jesus.Transparency = 1
    end
	end    
})

