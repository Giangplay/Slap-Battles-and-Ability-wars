
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({IntroText = "slap Royale",Name = "slap Royale", HidePremium = false, SaveConfig = true, ConfigFolder = "slap battles"})

--Anti chest bypass--

local namecall
	namecall =
	hookmetamethod(
	game,
	"__namecall",
	function(self, ...)
		local Args = {...}
		if getnamecallmethod() == "FireServer" and tostring(self) == "WS" then
			return
		elseif getnamecallmethod() == "FireServer" and tostring(self) == "WS2" then
			return
		elseif getnamecallmethod() == "FireServer" and tostring(self) == "AdminGUI" then
			return
		elseif getnamecallmethod() == "FireServer" and tostring(self) == "Ban" then
			return
		end
		return namecall(self, ...)
	end
	)

local Tab4 = Window:MakeTab({
	Name = "script",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab = Window:MakeTab({
	Name = "Item",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

