Tab4:AddToggle({
	Name = "get bob",
	Default = false,
	Callback = function(bool)
		GetBob = bool
        
            if bool == true then
                
                while GetBob do
            
            wait(.001)
            
             if not game.Players.LocalPlayer.Character:FindFirstChild("entered") and game.Players.LocalPlayer.leaderstats.Glove.Value == "Replica" then
            
                    wait(1)
                    
                    local humanoid = game.Players.LocalPlayer.Character.Humanoid

                    local HRP = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                    local Portal = game.Workspace.Lobby.Teleport1.Position

                    local path = pathfindingService:CreatePath()
 
                    path:ComputeAsync(HRP.Position, Portal)
 
                    local Waypoints = path:GetWaypoints()

                    repeat wait(.25)
                        for i, waypoint in pairs(Waypoints) do
                            
                            humanoid:MoveTo(waypoint.Position)
                            
                        end

                    until workspace[game.Players.LocalPlayer.Name]:FindFirstChild("entered")

                    wait(2)

                    if workspace[game.Players.LocalPlayer.Name]:FindFirstChild("entered") then
    
                        game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)

                        game:GetService("ReplicatedStorage").Duplicate:FireServer()

                    else
    
    
                    repeat wait(1.4)
    
                    until workspace[game.Players.LocalPlayer.Name]:FindFirstChild("entered") 

                    
                    game:GetService("ReplicatedStorage").Duplicate:FireServer()

    

                    end
                        
                        wait(1.45)

                        if not workspace:FindFirstChild("bobcap") then
    
                            game:GetService("ReplicatedStorage").HumanoidDied:FireServer(game.Players.LocalPlayer.Character, false)

                        else
        
                            print("Got Bob")
    
                        end
                
                    end 
           
                end
    
            end
	end    
})

Tab4:AddToggle({
	Name = "get tram",
	Default = false,
	Callback = function(bool)
		Brick = bool
        
        if bool == true then
            
            BooleanTing()
            
            end
       
    function BooleanTing()
        
              while Brick do
                  task.wait(0.01)
        if game.Players.LocalPlayer.leaderstats.Glove.Value == "Brick" and game.Players.LocalPlayer.Character:FindFirstChild("entered") then
        task.wait(5.2)
    
    game.VirtualInputManager:SendKeyEvent(true, "E", false, game)



end

end
        
    end   
	end    
})

