            BooleanTing()
            
            end
            
                      
   function BooleanTing()
        
              while bully do
        
        wait(.001)
    
    for _, v in ipairs(workspace:GetChildren()) do
    
    if string.sub(v.Name, 3, 8) == "Baller" then
game:GetService("ReplicatedStorage").GeneralHit:FireServer(v:WaitForChild("HumanoidRootPart"))
end
end
end
        
    end
	end    
